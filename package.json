{
  "name": "yayac",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/app",
    "server:prod": "node ./dist/app",
    "dev": "SET NODE_ENV=development && npm-run-all --parallel server client",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production && npm-run-all clean build && npm-run-all --parallel server:prod client:prod",
    "watch": "nodemon",
    "client:prod": "cd ./client && npm start",
    "client": "cd ./client && npm start"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "nodemon": "^1.19.0",
    "react-scripts": "^3.0.1"
  }
}
